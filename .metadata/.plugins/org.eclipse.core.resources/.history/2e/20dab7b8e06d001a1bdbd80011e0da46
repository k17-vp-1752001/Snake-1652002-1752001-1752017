package model;

import java.awt.Color;

public abstract class GameObject {
	
	protected int x, y; // object coordinates
	
	public GameObject(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public int getX() {
		return this.x;
	}
	
	public int getY() {
		return this.y;
	}
	
	public void setX(int x) {
		this.x = x;
	}
	
	public void setY(int y) {
		this.y = y;
	}
}
